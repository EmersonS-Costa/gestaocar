<!DOCTYPE html>
<html lang="pt-br">

<head> <!-- Inicio do Cabeçalho -->
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge"> <!-- defini como o navegador deve renderizar a página -->
  <meta name="viewport" content="width=device-width, initial-scale=1.0"> <!-- Torna a pagina responsiva -->
  <title>Usuario</title> <!-- Tag que adiciona o titulo na pagina -->
  <link rel="stylesheet" href="./css/style.css"> <!-- Link conexão ao arquivo CSS de estilos -->
  <script src="./js/js.js"></script> <!-- Link conexão ao arquivo JavaScript -->
  <!-- Links conexão ao Bootstrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-
    QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-
    I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-
    0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
</head>
<!-- Inicio do corpo da página -->
<body>
  <!-- Container com class container-fluid mt-4 e id contatos com class bootstrap -->
  <div class="container-fluid mt-4" id="contatos">
    <div class="col-12 text-center my-2">
      <h2 class="display-7 ">Perfil</h2>
    </div>
    <!-- seção com as divs,  -->
    <div id="dgvPostos" class="dgvPostos">
      <div id="dgvTitulos" class="dgvTitulos">
        <div class="U1">Nome</div>
        <div class="U2">CPF</div>
        <div class="U3">E-mail</div>
        <div class="U4">Endereço</div>
      </div>

      <div id="dgvDados" class="dgvDados">
        <div class="dgvLinhas">

          <div class="U1">
            <% usuario.forEach(function(item) { %> <!-- Código Javascript dentro do HTML  -->
              <li>
                <%= item.Nome %>
              </li>
              <% }); %>
          </div>

          <div class="U2">
            <% usuario.forEach(function(item) { %>
              <li>
                <%= item.CPF %>
              </li>
              <% }); %>
          </div>

          <div class="U3">
            <% usuario.forEach(function(item) { %>
              <li>
                <%= item.Email %>
              </li>
              <% }); %>
          </div>

          <div class="U4">
            <li>-</li>
          </div>

        </div>
      </div>
    </div>
  </div>
</body>

</html>